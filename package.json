{
  "name": "api-investiments",
  "version": "1.0.0",
  "description": "Api para integração com o banco de dados de investimentos",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/config/dataSource.ts migration:generate ./src/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/config/dataSource.ts migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adryanefernandes/api-investiments.git"
  },
  "keywords": [
    "investimentos",
    "FII",
    "Ações"
  ],
  "author": "Adryane Fernandes <edryfernandes@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adryanefernandes/api-investiments/issues"
  },
  "homepage": "https://github.com/adryanefernandes/api-investiments#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "mysql": "2.18.1",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.3.11"
  },
  "devDependencies": {
    "@types/express": "4.17.15",
    "@types/node": "18.11.18",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.4"
  }
}
